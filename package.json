{
  "name": "@hatsy/serve-static",
  "version": "2.1.0",
  "description": "Hatsy request handler that serves static content",
  "homepage": "https://github.com/hatsyjs/serve-static",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:hatsyjs/serve-static.git"
  },
  "license": "MIT",
  "author": "Ruslan Lopatin <ruslan.lopatin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hatsyjs/serve-static/issues"
  },
  "type": "module",
  "main": "./dist/serve-static.cjs",
  "module": "./dist/serve-static.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./dist/serve-static.js",
    "require": "./dist/serve-static.cjs"
  },
  "dependencies": {
    "send": "^0.17.1"
  },
  "peerDependencies": {
    "@hatsy/hatsy": "^2.2.0",
    "@hatsy/router": "^2.1.0"
  },
  "devDependencies": {
    "@hatsy/hatsy": "^2.2.0",
    "@hatsy/route-match": "^1.3.0",
    "@hatsy/router": "^2.1.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@run-z/eslint-config": "^1.2.1",
    "@run-z/rollup-helpers": "^1.1.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.7",
    "@types/send": "^0.14.7",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.2",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "rollup": "^2.44.0",
    "rollup-plugin-flat-dts": "^1.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "run-z": "^1.7.1",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.34",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "all": "run-z build,lint,test",
    "build": "run-z +z --then rollup --config ./rollup.config.js",
    "ci:all": "run-z all +test/--ci/--runInBand",
    "clean": "run-z +z --then shx rm -rf 'index.d.ts?(.map)' dist target",
    "doc": "run-z +z --then typedoc",
    "doc:publish": "run-z doc --then gh-pages --dist target/typedoc --dotfiles",
    "lint": "run-z +z --then eslint . --ext .js,.ts",
    "test": "run-z +z --then jest",
    "z": "run-z +build,+lint,+test,+doc"
  }
}
